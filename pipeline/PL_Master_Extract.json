{
	"name": "PL_Master_Extract",
	"properties": {
		"activities": [
			{
				"name": "Execute Extract",
				"type": "ExecutePipeline",
				"dependsOn": [
					{
						"activity": "GetConfiguration",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"pipeline": {
						"referenceName": "PL_Data_Extract",
						"type": "PipelineReference"
					},
					"waitOnCompletion": true,
					"parameters": {
						"RelativeUrlToRequest": {
							"value": "@activity('GetConfiguration').output",
							"type": "Expression"
						}
					}
				}
			},
			{
				"name": "GetConfiguration",
				"type": "SqlServerStoredProcedure",
				"state": "Inactive",
				"onInactiveMarkAs": "Succeeded",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"linkedServiceName": {
					"referenceName": "SQLPROD",
					"type": "LinkedServiceReference"
				}
			},
			{
				"name": "GetConfigError",
				"type": "Fail",
				"dependsOn": [
					{
						"activity": "GetConfiguration",
						"dependencyConditions": [
							"Failed"
						]
					}
				],
				"userProperties": [],
				"typeProperties": {
					"message": "Something went wrong during configuration extraction",
					"errorCode": {
						"value": "@activity('GetConfiguration').output",
						"type": "Expression"
					}
				}
			}
		],
		"variables": {
			"RestCMCApiKey": {
				"type": "String"
			}
		},
		"folder": {
			"name": "Extract"
		},
		"annotations": []
	}
}
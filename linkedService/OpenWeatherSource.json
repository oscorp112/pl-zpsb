{
	"name": "OpenWeatherSource",
	"properties": {
		"parameters": {
			"LocationLatitude": {
				"type": "string"
			},
			"LocationLongitude": {
				"type": "string"
			},
			"APIKey": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "RestService",
		"typeProperties": {
			"url": "https://api.openweathermap.org/data/2.5/weather?lat={LATPARAMETER}&lon={LONPARAMETER}&appid={APIKEY}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		}
	}
}